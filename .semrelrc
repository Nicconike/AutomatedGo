{
	"plugins": {
		"commit-analyzer": {
			"name": "default"
		},
		"changelog-generator": {
			"name": "default",
			"options": {
				"emojis": true
			}
		},
		"files-updater": {
			"name": "default",
			"files": [
				{
					"path": "CHANGELOG.md",
					"type": "changelog"
				},
				{
					"path": "go.mod",
					"type": "go-mod"
				}
			]
		}
	}
}
